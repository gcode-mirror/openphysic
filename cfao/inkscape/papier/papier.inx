<inkscape-extension>
  <_name>Papier</_name>
  <id>org.celles.filter.papier</id>
  <dependency type="executable" location="extensions">papier.py</dependency>
  <dependency type="executable" location="extensions">inkex.py</dependency>
<!--
	no param

  <param name="strTexte" type="string" _gui-text="Saisissez le texte a ecrire :">Papier</param>

  -->
  <effect>
    <object-type>all</object-type>
    <effects-menu>
       <submenu _name="celles.net"/>
    </effects-menu>
  </effect>
  <script>
    <command reldir="extensions" interpreter="python">papier.py</command>
  </script>
</inkscape-extension>
